// creates a new array with the length of 10
input "How many numbers would you like to store in an array? " and array myArray

var currIndex = 0
jsfeed currIndex
var arrayLength = [myArray length]
jsfeed arrayLength

while (js currIndex < arrayLength) {
    math currIndex + 1 and myArray set currIndex
    math currIndex + 1 and set currIndex
}

set currIndex 0

myArray foreach number {
    writeLine number
}