// creates a new array with the length of 10
array myArray 10

var currIndex = 0
jsfeed currIndex

:arrayFill
math currIndex + 1 and myArray set currIndex
math currIndex + 1 and set currIndex
if "currIndex < 10" "goto arrayFill"

set currIndex 0

:arrayOutput
myArray get currIndex and join "Number " currIndex " = " and writeLine
math currIndex + 1 and set currIndex
if "currIndex < 10" "goto arrayOutput"